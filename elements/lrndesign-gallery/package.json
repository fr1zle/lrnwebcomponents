{
  "name": "@lrnwebcomponents/lrndesign-gallery",
  "wcfactory": {
    "className": "LrndesignGallery",
    "customElementClass": "LitElement",
    "elementName": "lrndesign-gallery",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/lrndesign-gallery.css",
      "scss": "src/lrndesign-gallery.scss",
      "html": "src/lrndesign-gallery.html",
      "js": "src/lrndesign-gallery.js",
      "properties": "src/lrndesign-gallery-properties.json",
      "hax": "src/lrndesign-gallery-hax.json"
    },
    "sharedStyles": [
      "...super.styles"
    ]
  },
  "version": "2.6.8",
  "description": "displays images as a carousel or grid with the ability to zoom",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "lrndesign-gallery.js",
  "module": "lrndesign-gallery.es6.js",
  "umd": "lrndesign-gallery.umd.js",
  "amd": "lrndesign-gallery.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/lrndesign-gallery/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/img-pan-zoom": "^2.6.8",
    "@lrnwebcomponents/responsive-utility": "^2.6.8",
    "@lrnwebcomponents/simple-colors": "^2.6.8",
    "@lrnwebcomponents/simple-tooltip": "^2.6.8",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-image": "^3.0.0",
    "@polymer/paper-button": "^3.0.1",
    "lit-element": "^2.3.0"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "2.0.5",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.3",
    "concurrently": "5.2.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
